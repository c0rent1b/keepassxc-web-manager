{
  "name": "keepassxc-web-manager-frontend",
  "version": "2.0.0-alpha",
  "description": "Frontend build tools for KeePassXC Web Manager",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./frontend/src/css/tailwind.css -o ./frontend/public/css/tailwind.min.css --minify",
    "watch:css": "tailwindcss -i ./frontend/src/css/tailwind.css -o ./frontend/public/css/tailwind.min.css --watch",
    "dev": "npm run watch:css"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "alpinejs": "^3.13.5"
  }
}
